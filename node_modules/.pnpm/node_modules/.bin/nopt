#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/nopt.js/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/bin/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/nopt@1.0.10/node_modules/nopt/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/nopt@1.0.10/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules:/mnt/c/Users/Lil Nihilist/Projects/node_modules:/mnt/c/Users/Lil Nihilist/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/node_modules/nopt/bin/nopt.js/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/node_modules/nopt/bin/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/node_modules/nopt/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules:/mnt/c/Users/Lil Nihilist/Projects/node_modules:/mnt/c/Users/Lil Nihilist/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nopt/bin/nopt.js" "$@"
else 
  exec node  "$basedir/../nopt/bin/nopt.js" "$@"
fi
