#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

export NODE_PATH="/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/nodetouch.js/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/touch@3.1.0/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules/.pnpm/node_modules:/mnt/c/Users/Lil Nihilist/Projects/Node.js-Schwarzmueller-Assignment/node_modules:/mnt/c/Users/Lil Nihilist/Projects/node_modules:/mnt/c/Users/Lil Nihilist/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules"
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../touch@3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
else 
  exec node  "$basedir/../../../../../touch@3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
fi
